{
  "name": "slack-reaction-decomoji",
  "version": "5.0.0-preview",
  "description": "Fast, Fun, High-context communication. The perfect assets is this",
  "main": "",
  "scripts": {
    "update-md": "node scripts/generate_decomoji_md.js",
    "update-json": "node scripts/generate_decomoji_json.js",
    "import-basic": "cd scripts/ && bundle exec ruby import.rb",
    "import-extra": "cd scripts/ && bundle exec ruby import_extra.rb",
    "remove-basic": "cd scripts/ && bundle exec ruby remove.rb",
    "remove-extra": "cd scripts/ && bundle exec ruby remove_extra.rb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decomoji/slack-reaction-decomoji.git"
  },
  "author": "oti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decomoji/slack-reaction-decomoji/issues"
  },
  "homepage": "https://github.com/decomoji/slack-reaction-decomoji#readme",
  "dependencies": {
    "commander": "^5.1.0",
    "email-validator": "^2.0.4",
    "inquirer": "^7.1.0",
    "puppeteer": "^3.0.4"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*'",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
