{
  "name": "slack-reaction-decomoji",
  "version": "4.20.5",
  "description": "Fast, Fun, High-context communication. The perfect assets is this",
  "main": "",
  "scripts": {
    "prettier:fix": "prettier --write scripts/**/*.js",
    "update-md": "node scripts/generator/to_md.js",
    "update-json": "node scripts/generator/to_json.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decomoji/slack-reaction-decomoji.git"
  },
  "author": "oti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decomoji/slack-reaction-decomoji/issues"
  },
  "homepage": "https://github.com/decomoji/slack-reaction-decomoji#readme",
  "dependencies": {
    "commander": "^5.1.0",
    "email-validator": "^2.0.4",
    "inquirer": "^7.1.0",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|ts)'",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
